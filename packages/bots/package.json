		"date-fns": "^3.0.6",
		"discord.js": "^14.14.1",
		"dotenv-flow": "^4.0.1",
		"kysely": "^0.26.3",
		"kysely-migration-cli": "^0.2.3",
		"lodash": "^4.17.21",
		"node-fetch": "^3.3.2",
