  <head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width" />
    <link rel="icon" type="image/png" href="/favicon-96x96.png" />
    <title>{pageTitle}</title>
  </head>
  <body class="dark">
    <slot />
  </body>
</html>
